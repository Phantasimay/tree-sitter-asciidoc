{
  "name": "asciidoc",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_block"
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "SYMBOL",
          "name": "_admonitions"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "code"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "image"
        },
        {
          "type": "SYMBOL",
          "name": "url"
        },
        {
          "type": "SYMBOL",
          "name": "footnote"
        },
        {
          "type": "SYMBOL",
          "name": "kbd"
        },
        {
          "type": "SYMBOL",
          "name": "table"
        },
        {
          "type": "SYMBOL",
          "name": "xref"
        },
        {
          "type": "SYMBOL",
          "name": "description_list"
        },
        {
          "type": "SYMBOL",
          "name": "audio"
        },
        {
          "type": "SYMBOL",
          "name": "video"
        },
        {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      ]
    },
    "title": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "=+ .*\\n?"
        }
      ]
    },
    "_admonitions": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "note"
        },
        {
          "type": "SYMBOL",
          "name": "tip"
        },
        {
          "type": "SYMBOL",
          "name": "important"
        },
        {
          "type": "SYMBOL",
          "name": "caution"
        },
        {
          "type": "SYMBOL",
          "name": "warning"
        }
      ]
    },
    "note": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "NOTE:"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": " "
                },
                {
                  "type": "PATTERN",
                  "value": ".*"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "tip": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "TIP: "
        },
        {
          "type": "PATTERN",
          "value": ".+"
        }
      ]
    },
    "important": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "IMPORTANT: "
        },
        {
          "type": "PATTERN",
          "value": ".+"
        }
      ]
    },
    "caution": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "CAUTION: "
        },
        {
          "type": "PATTERN",
          "value": ".+"
        }
      ]
    },
    "warning": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "WARNING: "
        },
        {
          "type": "PATTERN",
          "value": ".+"
        }
      ]
    },
    "list": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "_list_item"
        }
      }
    },
    "_list_item": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\*+"
            },
            {
              "type": "PATTERN",
              "value": "-+"
            },
            {
              "type": "PATTERN",
              "value": "\\.+"
            }
          ]
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "PATTERN",
          "value": ".+"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "code": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[,"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": " "
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "language",
          "content": {
            "type": "PATTERN",
            "value": "\\w+"
          }
        },
        {
          "type": "STRING",
          "value": "]\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": ".*"
          }
        },
        {
          "type": "STRING",
          "value": "----\n"
        },
        {
          "type": "PATTERN",
          "value": "----\\n?"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "// "
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "image": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "image::"
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "PATTERN",
            "value": "[\\w./:]*"
          }
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "title",
          "content": {
            "type": "PATTERN",
            "value": "[\\w.]*"
          }
        },
        {
          "type": "STRING",
          "value": "]\n"
        }
      ]
    },
    "url": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-z]+:\\/\\/[^\\s]*"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "["
                },
                {
                  "type": "PATTERN",
                  "value": "[\\w.]*"
                },
                {
                  "type": "STRING",
                  "value": "]"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "footnote": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "footnote:["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[\\w._]+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "kbd": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "kbd:["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\w+(\\+\\w+)?"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "table": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "|===\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": ".*"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\|===\\n?"
        }
      ]
    },
    "xref": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<<"
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "PATTERN",
            "value": "\\w+"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "FIELD",
                  "name": "title",
                  "content": {
                    "type": "PATTERN",
                    "value": "\\w+"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ">>"
        }
      ]
    },
    "description_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\w+"
        },
        {
          "type": "STRING",
          "value": ":: "
        },
        {
          "type": "PATTERN",
          "value": ".+"
        }
      ]
    },
    "audio": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "audio::"
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[\\w.]*"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "title",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[\\w.]+"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "video": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "video::"
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[\\w.]*"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "title",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[\\w.]+"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_inline_element"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "_inline_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "emphasis"
        },
        {
          "type": "SYMBOL",
          "name": "strong"
        },
        {
          "type": "SYMBOL",
          "name": "monospace"
        },
        {
          "type": "SYMBOL",
          "name": "superscript"
        },
        {
          "type": "SYMBOL",
          "name": "subscript"
        },
        {
          "type": "PATTERN",
          "value": "\\w+"
        }
      ]
    },
    "emphasis": {
      "type": "PATTERN",
      "value": "_\\w+_"
    },
    "strong": {
      "type": "PATTERN",
      "value": "\\*\\w+\\*"
    },
    "monospace": {
      "type": "PATTERN",
      "value": "`\\w+`"
    },
    "superscript": {
      "type": "PATTERN",
      "value": "\\^\\w+\\^"
    },
    "subscript": {
      "type": "PATTERN",
      "value": "~\\w~"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

