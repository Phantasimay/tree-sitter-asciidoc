{
  "name": "asciidoc",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_block"
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "SYMBOL",
          "name": "note"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "code"
        },
        {
          "type": "SYMBOL",
          "name": "note"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "image"
        },
        {
          "type": "SYMBOL",
          "name": "url"
        },
        {
          "type": "SYMBOL",
          "name": "footnote"
        }
      ]
    },
    "title": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "=+"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "note": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "NOTE:"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": " "
                },
                {
                  "type": "PATTERN",
                  "value": ".*"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[\\-.]{1}"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "code": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[,"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": " "
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "language",
          "content": {
            "type": "PATTERN",
            "value": "\\w+"
          }
        },
        {
          "type": "STRING",
          "value": "]\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": ".*"
          }
        },
        {
          "type": "STRING",
          "value": "----\n"
        },
        {
          "type": "STRING",
          "value": "----"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "// "
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "image": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "image::"
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "PATTERN",
            "value": "[\\w./:]*"
          }
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "title",
          "content": {
            "type": "PATTERN",
            "value": "[\\w.]*"
          }
        },
        {
          "type": "STRING",
          "value": "]\n"
        }
      ]
    },
    "url": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-z]+:\\/\\/[^\\s]*"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "["
                },
                {
                  "type": "PATTERN",
                  "value": "[\\w.]*"
                },
                {
                  "type": "STRING",
                  "value": "]"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "footnote": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "footnote:["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[\\w._]+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

